{
  "name": "rescript-static",
  "private": true,
  "scripts": {
    "postinstall": "npm run re:build",
    "re:build": "rescript build",
    "re:watch": "rescript build -w",
    "blog:build": "node scripts/build -- ./docs",
    "blog:serve": "node scripts/serve -- ./docs",
    "blog:watch": "watch 'npm run blog:build -s' ./content ./src ./static",
    "dev": "npm run blog:serve & npm run blog:watch"
  },
  "dependencies": {
    "browser-sync": "^2.26.14",
    "copyfiles": "^2.4.1",
    "front-matter": "^4.0.2",
    "glob": "^7.1.6",
    "highlight.js": "^10.7.2",
    "linkify-it": "^3.0.2",
    "markdown-it": "^12.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reason-react": "^0.9.1",
    "rescript": "^9.1.2",
    "rimraf": "^3.0.2",
    "watch": "^1.0.2",
    "yargs": "^17.0.1"
  }
}
